#lang web-server/insta
(define (start request)
  (response/xexpr
   '(html
     (head (title "My Blog"))
     (body (h1 "Under Construction")))))

(struct post (title:string? body:string?))
(struct blog (listof post?))

(define Post1 (post "Title1" "Body1"))
(define Post3 (post "Title3" "Body3"))
(define Blog (list Post1 (post "S1" "C1") Post3))

(define xexpr/c
  (flat-rec-contract
   xexpr
   (or/c string?
         (cons/c symbol? (listof xexpr))
         (cons/c symbol?
                 (cons/c (listof (list/c symbol? string?))
                         (listof xexpr))))))